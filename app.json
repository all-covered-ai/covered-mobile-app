{
  "expo": {
    "name": "Covered",
    "slug": "covered-mobile-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.allcovered.mobile"
    },
    "android": {
      "package": "com.allcovered.mobile",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "plugins": [
      "expo-notifications",
      "expo-secure-store"
    ],
    "scheme": "covered",
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
